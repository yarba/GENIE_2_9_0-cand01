<?xml version="1.0" encoding="ISO-8859-1"?>

<!--
Configuration for the RESInteractionListGenerator InteractionListGeneratorI

Configurable Parameters:
..........................................................................................................................
Name                        Type    Opt  Comment                                               Default
..........................................................................................................................
ResonanceNameList           string  Yes  list of resonances to be taken into account           GPL: ResonanceNameList
is-CC                       bool    Yes  set true for CC list                                  false
is-NC                       bool    Yes  set true for NC list                                  false
is-EM                       bool    Yes  set true for EM list                                  false
-->

<alg_conf>

  <param_set name="CC-Default">
    <param type="bool" name="is-CC"> true </param>
  </param_set>

  <param_set name="NC-Default">
    <param type="bool" name="is-NC"> true </param>
 </param_set>

  <param_set name="EM-Default">
    <param type="bool" name="is-EM"> true </param>
 </param_set>

</alg_conf>

